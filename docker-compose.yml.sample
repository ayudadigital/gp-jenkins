version: '2.4'
services:
  jenkins:
    image: ayudadigital/gp-jenkins:2.10.0
    build: jenkins
    ports:
      - 8080:8080
    privileged: true
    volumes:
      - ./data/var/jenkins_home:/var/jenkins_home
      - docker:/var/lib/docker

networks:
    default:
      external:
        name: platform_services

volumes:
  docker:
